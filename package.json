{
  "name": "mock-ecommerce-api",
  "version": "1.0.0",
  "description": "Mock e-commerce platform API for AI agent hiring tests",
  "main": "dist/server.js",
  "scripts": {
    "build": "tsc",
    "start": "node dist/server.js",
    "dev": "ts-node src/server.ts",
    "seed": "ts-node src/seed.ts",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "adk:web": "npx adk web src/agent/root.agent.ts --port 8000",
    "adk:run": "npx adk run src/agent/root.agent.ts"
  },
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@google/adk": "^0.3.0",
    "@google/adk-devtools": "^0.3.0",
    "express": "^4.18.2",
    "groq-sdk": "^0.37.0",
    "uuid": "^9.0.0"
  },
  "devDependencies": {
    "@types/express": "^4.17.21",
    "@types/jest": "^29.5.11",
    "@types/node": "^20.10.0",
    "@types/supertest": "^6.0.2",
    "@types/uuid": "^9.0.7",
    "jest": "^29.7.0",
    "supertest": "^6.3.3",
    "ts-jest": "^29.1.1",
    "ts-node": "^10.9.2",
    "typescript": "^5.9.3"
  }
}
